yId(id)\n      .then(() => {\n        const remainderBooks = userBooks.filter((book) => book._id !== id);\n        setUserBooks(remainderBooks);\n      })\n      .catch((err) => console.log(err));\n  };\n\n  return (\n    <div className='mt-6 columns is-flex is-flex-wrap-wrap'>\n      {userBooks.map((book) => (\n        <div className='column is-6 has-text-centered'>\n        <div className='box border-gray' key={book._id}>\n          <Link to={`/book/read/${book._id}`}>\n          <h1 className='is-size-3'>{book.title}</h1>\n          </Link>\n          </div>\n          { props.user && book.owner === props.user._id && (\n            <button className='button is-danger is-light' onClick={() => deleteBook(book._id)}>\n              Delete book\n            </button>\n          )}\n        </div>\n      ))}\n    </div>\n  );\n};\n\nexport default UserBooks;","/Users/alex/Desktop/Ironhack/week8/adventure/client/src/components/Root/Home.js",["69"],"/Users/alex/Desktop/Ironhack/week8/adventure/client/src/components/Footer/Footer.js",[],{"ruleId":"70","replacedBy":"71"},{"ruleId":"72","replacedBy":"73"},{"ruleId":"74","severity":1,"message":"75","line":25,"column":13,"nodeType":"76","endLine":25,"endColumn":29},{"ruleId":"77","severity":1,"message":"78","line":24,"column":1,"nodeType":"79","endLine":29,"endColumn":3},{"ruleId":"80","severity":1,"message":"81","line":17,"column":27,"nodeType":"82","endLine":17,"endColumn":29,"suggestions":"83"},{"ruleId":"74","severity":1,"message":"75","line":11,"column":9,"nodeType":"76","endLine":11,"endColumn":42},"no-native-reassign",["84"],"no-negated-in-lhs",["85"],"jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement","import/no-anonymous-default-export","Assign object to a variable before exporting as module default","ExportDefaultDeclaration","react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'props.user._id'.